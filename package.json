{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "biome format . --write",
    "lint": "biome lint . --apply",
    "check": "biome check . --write",
    "generate-sitemap": "node generate-sitemap.cjs",
    "component": "generate-react component",
    "page": "generate-react component  --type=page",
    "layout": "generate-react component  --type=layout",
    "redux": "generate-react component  --type=redux",
    "bundle-analyze": "vite-bundle-visualizer",
    "prepare": "husky install",
    "init-husky": "npx husky-init",
    "test": "npx lint-staged"
  },
  "browserslist": {
    "production": [
      "iOS >= 9",
      "Android >= 4.4",
      "last 2 versions",
      "> 0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@million/lint": "^1.0.0-rc.81",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "jwt-decode": "^4.0.0",
    "million": "^3.1.11",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "sharp": "^0.33.4",
    "styled-components": "^6.1.11",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "generate-react-cli": "^8.4.6",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "vite": "^5.2.13",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-imagetools": "^7.0.2",
    "vite-plugin-compression2": "^1.1.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-vitals": "^1.0.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check . --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ],
    "src/**/*.css": [
      "biome format . --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  }
}
